		<script type="text/javascript" src="https://cdn.datatables.net/v/dt/dt-1.10.18/datatables.min.js"></script>
		<script type="text/javascript" src="{{ constant('App\\Controller\\MainController::BASE_URL') }}assets/node_modules/accounting/accounting.js"></script>

